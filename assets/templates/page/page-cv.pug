<div class="cv-portfolio" id="portfolio">
	<div class="wrapper-left">
		<input type="checkbox" id="cv-toggle" autocomplete="off">
		<label class="cv-toggle" for="cv-toggle">
			<div class="the-cv">
				include ../partials/cv/header.pug
				include ../partials/cv/experience.pug
				include ../partials/cv/summary.pug
				<hr>
				include ../partials/cv/work.pug
				<hr>
				include ../partials/cv/hobby.pug
				<hr>
				include ../partials/cv/cta.pug
			</div>
			<label class="cv-toggle-close" for="cv-toggle">x</label>
			<button onclick="window.print()" aria-label="Print the page" class="cv-download-button-wrapper"><span class="cv-download-button">print<br>copy</span>
			</button>
		</label>
	</div>
	<div class="wrapper-right">
		include ../partials/cv/content.pug
	</div>
</div>