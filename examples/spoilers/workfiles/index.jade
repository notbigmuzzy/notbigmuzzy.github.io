		//- HEADER AND BUTTONS
		div.header
			div.head-close
				div.fa.fa-times
			div.head-menu
				div.fa.fa-align-justify 
			div.thisguy-close
				div.fa.fa-times
			div.thisguy-closetrans
				div.fa.fa-times
		div.button-wrapper
			div.button-left
				div.fa.fa-chevron-left
			div.button-right
